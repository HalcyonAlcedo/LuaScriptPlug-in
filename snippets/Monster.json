{
	"设置怪物筛选器": {
		"prefix": "Game_Monster_SetFilter",
		"body": ["Game_Monster_SetFilter(${1:id},${0:subid})"],
		"description": "设置怪物筛选器"
	},
	"清除怪物筛选器": {
		"prefix": "Game_Monster_DisableFilter",
		"body": ["Game_Monster_DisableFilter()"],
		"description": "清除环境生物筛选器"
	},
	"设置导航标记怪物行为": {
		"prefix": ["SetBehaviorOfNavigationMonsters","Game_Monster_SetBehaviorOfNavigationMonsters"],
		"body": ["Game_Monster_SetBehaviorOfNavigationMonsters(${0:id})"],
		"description": "设置导航标记的怪物的行为,不受筛选器影响"
	},
	"杀死导航标记怪物": {
		"prefix": ["KillNavigationMarkMonster","Game_Monster_KillNavigationMarkMonster"],
		"body": ["Game_Monster_KillNavigationMarkMonster()"],
		"description": "杀死导航标记的怪物,受筛选器影响"
	},
