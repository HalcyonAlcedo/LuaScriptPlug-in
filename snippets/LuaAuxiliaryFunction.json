		"prefix": ["DestroyVariable","Lua_Variable_DestroyVariable"],
		"body": ["Lua_Variable_DestroyVariable('${0:name}')"],
		"description": "销毁变量"
	},
	"获取随机数": {
		"prefix": "Lua_Math_Random",
		"body": ["Lua_Math_Random(${1:min},${0:max})"],
		"description": "获取随机数"
	},
	"设置子脚本命名前缀": {
		"prefix": "Lua_Variable_SetSubScriptVariablePrefix",
		"body": ["Lua_Variable_SetSubScriptVariablePrefix('${0:name}')"],
		"description": "设置子脚本命名前缀"
	},
	"获取UUID": {
		"prefix": "System_GetUUID",
		"body": ["System_GetUUID()"],
		"description": "获取UUID"
	},
	"播放音频文件": {
		"prefix": "System_Sound_PlaySound",
		"body": ["System_Sound_PlaySound('${0:filePath}')"],
		"description": "播放音频文件"
	}
