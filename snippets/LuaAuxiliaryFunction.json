	},
	"读取全局浮点数变量": {
		"prefix": ["ReadGlobalFloatVariable","Lua_Variable_ReadGlobalFloatVariable"],
		"body": ["Lua_Variable_ReadGlobalFloatVariable('${0:name}')"],
		"description": "读取全局浮点数变量"
	},
	"读取全局字符串变量": {
		"prefix": ["ReadGlobalStringVariable","Lua_Variable_ReadGlobalStringVariable"],
