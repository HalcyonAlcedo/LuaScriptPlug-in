		"body": ["Lua_Variable_SaveGlobalFloatVariable('${1:name}',${0:value})"],
		"description": "存入全局浮点数变量"
	},
	"存入全局字符串变量": {
		"prefix": ["SaveGlobalStringVariable","Lua_Variable_SaveGlobalStringVariable"],
		"body": ["Lua_Variable_SaveGlobalStringVariable('${1:name}',${0:value})"],
