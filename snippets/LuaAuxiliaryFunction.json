{
	"存入整数变量": {
		"prefix": ["SaveIntVariable","Lua_Variable_SaveIntVariable"],
		"body": ["Lua_Variable_SaveIntVariable('${1:name}',${0:value})"],
		"description": "存入整数变量"
	},
	"存入浮点数变量": {
		"prefix": ["SaveFloatVariable","Lua_Variable_SaveFloatVariable"],
		"body": ["Lua_Variable_SaveFloatVariable('${1:name}',${0:value})"],
		"description": "存入浮点数变量"
	},
	"存入字符串变量": {
		"prefix": ["SaveStringVariable","Lua_Variable_SaveStringVariable"],
		"body": ["Lua_Variable_SaveStringVariable('${1:name}',${0:value})"],
		"description": "存入字符串变量"
	},
